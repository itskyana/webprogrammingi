{
    "version": 2,
    "routes": [
      {
        "src": "/assets/(.*)",
        "dest": "/assets/$1"
      },
      {
        "src": "/template/(.*)",
        "dest": "/template/$1"
      },
      {
        "src": "/config/(.*)",
        "dest": "/config/$1"
      },
      {
        "src": "/pages/(.*)",
        "dest": "/pages/$1"
      },
      {
        "src": "/(.*)",
        "dest": "/index.php"
      }
    ]
  }
  